{
    "rules": {
        "no-unused-vars": [0],
         "key-spacing": [0, {
             "align": "colon"
         }],
        "quotes": [
            0,
            "double"
        ],
        "linebreak-style": [
            0,
            "unix"
        ],
        "semi": [
            0,
            "always"
        ],
        "no-wrap-func": [0]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jquery": true
    },
    "globals": {
        "_": true
    }
}